{
  "name": "project-name-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "db:generate": "supabase gen types typescript --project-id $SUPABASE_PROJECT_ID --schema public > src/types/supabase.ts",
    "db:reset": "supabase db reset",
    "db:push": "supabase db push",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "test:visual": "playwright test tests/visual --reporter=html",
    "test:examples": "playwright test tests/examples --reporter=html",
    "init": "node scripts/initialize-template.js",
    "qvalidate": "npm run type-check && npm run lint && npm run test && npm run test:e2e",
    "qcheck": "npm run type-check && npm run lint && npm run format:check",
    "prebuild": "npm run qcheck",
    "setup:dev": "npm install && npm run qcheck && npm run test",
    "visual:update": "playwright test tests/visual --update-snapshots",
    "quality:full": "npm run type-check && npm run lint && npm run test && npm run test:visual && npm run build"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/supabase-js": "^2.39.0",
    "@use-gesture/react": "^10.3.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.263.1",
    "next": "15.4.6",
    "react": "^18.2.0",
    "react-audio-player": "^0.17.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-spring": "^9.7.3",
    "stripe": "^14.15.0",
    "tailwind-merge": "^2.2.0",
    "type-fest": "^4.10.2",
    "wavesurfer.js": "^7.7.3",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^14.3.1",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/wavesurfer.js": "^6.0.7",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.17.0",
    "autoprefixer": "^10.4.16",
    "dotenv": "^17.2.1",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.4.6",
    "fast-check": "^3.15.1",
    "jest": "^29.7.0",
    "postcss": "^8.4.32",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "supabase": "^2.34.3",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3"
  }
}
